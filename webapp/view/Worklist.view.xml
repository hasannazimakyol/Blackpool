<mvc:View controllerName="mbis.Blackpool.controller.Worklist" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.f.semantic">
	<semantic:SemanticPage id="page" headerPinnable="false" toggleHeaderOnTitleClick="false">
		<semantic:titleHeading>
			<Title text="{i18n>worklistTitle}"/>
		</semantic:titleHeading>
		<semantic:content>
			<VBox>
				<Panel>
					<mvc:XMLView viewName="mbis.Blackpool.view.Header"/>
				</Panel>
			<mvc:XMLView viewName="mbis.Blackpool.view.PlayersList"/>
			</VBox>
		</semantic:content>
	</semantic:SemanticPage>
</mvc:View>